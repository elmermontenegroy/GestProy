<?xml version="1.0" encoding="UTF-8" ?>

<!DOCTYPE struts PUBLIC
        "-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
        "http://struts.apache.org/dtds/struts-2.3.dtd">

<struts>
	<package name="struts-portafolio" extends="demo" namespace="/portafolio">
	
		<action name="inicializarPortafolio" class="com.gestproy.actions.PortafolioAction">
			<result type="tiles">d_portafolio_listar</result>
		</action>
		
		<action name="cargarRegistrarPortafolio" class="com.gestproy.actions.PortafolioAction" method="cargarRegistrar">
			<result type="tiles">d_portafolio_registrar</result>
		</action>
		
		<action name="registrarPortafolio" class="com.gestproy.actions.PortafolioAction" method="registrar">
			<result type="tiles">d_portafolio_listar</result>
		</action>
		
		<action name="cargarActualizarPortafolio" class="com.gestproy.actions.PortafolioAction" method="cargarActualizar">
			<result type="tiles">d_portafolio_actualizar</result>
		</action>
		
		<action name="actualizarPortafolio" class="com.gestproy.actions.PortafolioAction" method="actualizar">
			<result type="tiles">d_portafolio_listar</result>
		</action>
		
		<action name="eliminarPortafolio" class="com.gestproy.actions.PortafolioAction" method="eliminar">
			<result type="tiles">d_portafolio_listar</result>
		</action>
		
		
		
	</package>
</struts>